        {
            "name": "Python: test_net.py",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--num-gpus",
                "1",
                "--config-file",
                "checkpoints/voc/faster_rcnn/faster_rcnn_R_101_FPN_base1_distill_g1/config.yaml",
                "--eval-all",
            ]
        },
        {
            "name": "Python: train distill",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--num-gpus",
                "1",
                "--config-file",
                "configs/PascalVOC-detection/split1/faster_rcnn_R_101_FPN_base1.yaml",
                "--path_t",
                "checkpoints/voc/faster_rcnn/faster_rcnn_R_101_FPN_base1/model_final.pth",
                "--kd_T",
                "4",
                "OUTPUT_DIR",
                "checkpoints/voc/faster_rcnn/faster_rcnn_R_101_FPN_base1_distill_wogtroi_g1",
                "SOLVER.IMS_PER_BATCH",
                "2",
                "SOLVER.BASE_LR",
                "0.00025",
                "SOLVER.STEPS",
                "(96000, 128000)",
                "SOLVER.MAX_ITER",
                "200",
                "SOLVER.WARMUP_ITERS",
                "800",
